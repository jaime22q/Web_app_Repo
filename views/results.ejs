<% layout('./layout/boilerplate') -%>
<h2>Results</h2>
<div class="container2">
  <% response["data"].items.forEach(item => { %>
      <div class="thumbnail">
        <img src= <%=
        item.volumeInfo.imageLinks === undefined ? "https://dubsism.files.wordpress.com/2017/12/image-not-found.png" : item.volumeInfo.imageLinks.thumbnail %> >
        <div class="caption">
        <%= item.volumeInfo["title"] %>
        <span> - </span>
        <%= item.volumeInfo["authors"] %>
          </div>
        </div>
  <% }) %>
</div>